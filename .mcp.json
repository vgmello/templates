{
	"mcpServers": {
		"playwright": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@playwright/mcp@latest",
				"--browser",
				"chromium",
				"--headless",
				"--no-sandbox",
				"--output-dir",
				"_temp/playwright-screenshots"
			],
			"env": {}
		},
		"context7": {
			"type": "stdio",
			"command": "npx",
			"args": ["-y", "@upstash/context7-mcp"],
			"env": {}
		}
	}
}
