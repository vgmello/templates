		"shadcn-svelte": "^1.0.5",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^3.4.17",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.2.6",
