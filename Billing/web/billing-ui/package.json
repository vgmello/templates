		"@sveltejs/adapter-auto": "^6.0.1",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "^22.16.0",
		"@vitest/browser": "^3.2.4",
		"autoprefixer": "^10.4.21",
